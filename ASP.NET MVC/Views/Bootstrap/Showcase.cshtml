                    &lt;option data-subtext="Speck"&gt;Bacon&lt;/option&gt;
                    &lt;option&gt;Ham&lt;/option&gt;
                    &lt;option&gt;Egg&lt;/option&gt;
                &lt;/optgroup&gt;
            &lt;/select&gt;
